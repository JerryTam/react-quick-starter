{
  "//printWidth 限制每行字符个数": 0,
  "printWidth": 80,
  "//tabWidth 指定每个缩进级别的空格数": 0,
  "tabWidth": 2,
  "//useTabs 使用制表符而不是空格缩进": 0,
  "useTabs": false,
  "//semi 在语句末尾打印分号": 0,
  "semi": true,
  "//singleQuote 使用单引号而不是双引号": 0,
  "singleQuote": true,
  "//trailingComma 多行时尽可能打印尾随逗号": 0,
  "trailingComma": "es5",
  "//bracketSpacing 在对象文字中的括号之间打印空格": 0,
  "bracketSpacing": true,
  "//arrowParens 始终给箭头函数的参数加括号": 0,
  "arrowParens": "always",
  "//htmlWhitespaceSensitivity 指定HTML文件的全局空格敏感度": 0,
  "htmlWhitespaceSensitivity": "css",
  "//endOfLine 检测换行符类型，如果出现大量换行符报错，可以修改为auto不检测": 0,
  "endOfLine": "lf"
}
